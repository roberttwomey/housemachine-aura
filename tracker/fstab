proc            /proc           proc    defaults          0       0
PARTUUID=ae8d534a-01  /boot           vfat    defaults          0       2
PARTUUID=ae8d534a-02  /               ext4    defaults,noatime  0       1
PARTUUID=d2ec33c3-01 /media/housedata ext4 defaults,auto,umask=000,users,rw,nofail	0 0
